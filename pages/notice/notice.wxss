/* pages/notice/notice.wxss */
.notice {
}
.notice_box {
  padding: 30rpx;
}
.notice_box_title {
  display: block;
  text-align: center;
  padding-top: 20rpx;
  border-bottom: 2rpx solid #ddd;
  color: #333333;
  font-size: 30rpx;
  padding-bottom: 20rpx;
}
.notice_box_time {
  display: block;
  text-align: center;
  padding-top: 10rpx;
  font-size: 28rpx;
}
.notice_box_name {
  font-size: 28rpx;
  color: #333333;
  padding-top: 10rpx;
}
.notice_box_img {
  width: 100%;
}
.notice_box_img image {
  width: 100%;
  height: 750rpx;
}